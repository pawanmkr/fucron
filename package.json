        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "typescript": "^5.x"
    },
    "dependencies": {
        "tsup": "^8.5.0"
    },
    "keywords": [
        "scheduler",
        "timeout",
        "task",
        "job",
        "delay",
        "timer",
