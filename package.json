{
    "name": "@pawanmkr/fucron",
    "version": "0.0.1",
    "type": "module",
    "main": "./dist/index.js",
    "module": "dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
